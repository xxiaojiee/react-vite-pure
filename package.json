{
  "name": "vite-react-app",
  "version": "0.1.0",
  "scripts": {
    "bootstrap": "pnpm install",
    "start": "npm run dev",
    "dev": "vite",
    "build": "cross-env NODE_ENV=production vite build && esno ./build/script/postBuild.ts",
    "build:no-cache": "pnpm clean:cache && npm run build",
    "report": "cross-env REPORT=true npm run build",
    "preview": "npm run build && vite preview",
    "preview:dist": "vite preview",
    "log": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "reinstall": "rimraf yarn.lock && rimraf package.lock.json && rimraf node_modules && npm run bootstrap",
    "zeslint-scan": "zeslint scan",
    "zeslint-fix": "zeslint fix"
  },
  "dependencies": {
    "@ant-design/charts": "^1.0.21",
    "@ant-design/icons": "^4.5.0",
    "@ant-design/pro-descriptions": "^1.6.6",
    "@ant-design/pro-layout": "^6.15.2",
    "@ant-design/pro-table": "^2.30.6",
    "@zxcvbn-ts/core": "^1.2.0",
    "ahooks": "^2.9.6",
    "antd": "^4.13.1",
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "cross-env": "^7.0.3",
    "crypto-js": "4.1.1",
    "esno": "^0.5.0",
    "immer": "^8.0.1",
    "lodash-es": "^4.17.21",
    "mockjs": "^1.1.0",
    "qrcode": "^1.5.0",
    "qs": "^6.10.1",
    "query-string": "^7.0.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-redux": "^7.2.3",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "unstated-next": "^1.1.0",
    "use-immer": "^0.4.2"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^5.3.0",
    "@types/crypto-js": "4.0.2",
    "@types/lodash-es": "^4.17.5",
    "@types/mockjs": "^1.0.4",
    "@types/node": "^14.14.32",
    "@types/qs": "6.9.7",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@types/redux": "^3.6.0",
    "@vitejs/plugin-legacy": "^1.3.2",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "conventional-changelog-cli": "^2.1.1",
    "dotenv": "^8.2.0",
    "global": "^4.4.0",
    "less": "^4.1.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.2",
    "vite": "^2.0.5",
    "vite-plugin-html": "2.0.7",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-style-import": "^0.8.1",
    "vite-plugin-svg-icons": "1.0.4",
    "vite-plugin-theme": "^0.8.1",
    "vite-plugin-windicss": "^1.6.1",
    "zeslint": "^0.1.3"
  },
  "engines": {
    "node": ">= 12.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "zeslint commit-file-scan",
      "commit-msg": "zeslint commit-msg-scan"
    }
  }
}
